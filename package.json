{
  "name": "reid-viewer",
  "displayName": "ReID Viewer",
  "description": "ReID visualization tool with cosine similarity display",
  "version": "0.0.3",
  "publisher": "local",
  "repository": {
    "type": "git",
    "url": "https://github.com/example/reid-viewer"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:reidViewer.viewImages"
  ],
  "contributes": {
    "commands": [
      {
        "command": "reidViewer.viewImages",
        "title": "ReID Viewer"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "reidViewer.viewImages",
          "title": "ReID Viewer: Open"
        }
      ],
      "explorer/context": [
        {
          "command": "reidViewer.viewImages",
          "title": "ReID Viewer",
          "group": "reidviewer@1",
          "when": "explorerResourceIsFolder && !listMultiSelection"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}